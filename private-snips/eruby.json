{
  "js_pack_tag": {
    "prefix": ["jspt"],
    "body": [
      "<%= javascript_pack_tag('${1}') %>"
    ]
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "<% if ${1:statement} %>",
      "\t${0}",
      "<% end %>"
    ]
  },
  "do": {
    "prefix": ["do"],
    "body": [
      "<%= ${1} do %>",
      "\t${0}",
      "<% end %>"
    ]
  }
}
