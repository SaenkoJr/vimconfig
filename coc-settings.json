{
  "coc.preferences.jumpCommand": "split",

  "suggest.timeout": 500,
  "suggest.maxCompleteItemCount": 30,
  "suggest.maxPreviewWidth": 100,
  "suggest.detailField": "preview",
  "suggest.removeDuplicateItems": true,

  "eslint.packageManager": "yarn",
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "javascript.jsx"
  ],

  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.excludeLanguages": ["markdown", "javascript", "javascriptreact"],
  "coc.source.neosnippet.priority": 90,
  "suggest.languageSourcePriority": 100,

  "diagnostic.enable": false,
  "diagnostic.refreshOnInsertMode": false,

  "javascript.suggest.autoImports": true,
  "javascript.suggestionActions.enabled": false,

  "typescript.suggest.autoImports": true,
  "typescript.preferences.quoteStyle": "single",

  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  }
}
